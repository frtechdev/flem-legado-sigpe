        </dependency>
        
        <dependency>
            <groupId>net.sf.jasperreports</groupId>
            <artifactId>jasperreports-htmlcomponent</artifactId>
            <version>4.1.1</version>
        </dependency>

        <dependency>
            <groupId>sybase.jdbc4.sqlanywhere</groupId>
            <artifactId>sajdbc4</artifactId>
            <!-- Windows-->
            <!--<version>16.0.0.2193</version>-->
            <!-- Linux -->
            <version>16.0.0.1948</version> 
